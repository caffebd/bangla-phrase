[application]

config/name="Mystery Word"
run/main_scene="res://scenes/menu.tscn"
boot_splash/image="res://assets/menu/splash.png"
config/icon="res://icon.png"

[autoload]

SharedVariables="*res://script/shared_variables.gd"
GlobalSignals="*res://script/GlobalSignals.gd"
Firebase="*res://addons/godot-firebase/firebase/firebase.tscn"
UserData="*res://script/user_data.gd"
GameMusic="*res://scenes/GameMusic.tscn"
MobileAds="*res://addons/admob/src/singletons/MobileAds.gd"

[display]

window/size/width=1080
window/size/height=1920
window/size/test_width=720
window/size/test_height=1080
window/dpi/allow_hidpi=true
window/handheld/orientation="portrait"
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "res://addons/admob/plugin.cfg", "res://addons/godot-firebase/plugin.cfg" )

[gui]

common/drop_mouse_on_gui_input_disabled=true

[input_devices]

pointing/emulate_touch_from_mouse=true

[network]

limits/debugger_stdout/max_chars_per_second=204800

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_clear_color=Color( 0.215686, 0.25098, 0.72549, 1 )
environment/default_environment="res://default_env.tres"
